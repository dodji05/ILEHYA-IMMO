{% extends 'FrontEnd/base.html.twig' %}
{% block style%}
    <style>
        section {
            padding: 35px 0 !important;
        }

        /*.owl-next, .owl.prev {*/
        /*    margin:35px;*/
        /*}*/

        .card__image-body h6 a {

            /* font-size: 15px */
            font-size: 12px !important;
        }

        .link__overlay {
            border-radius: 4px;
            display: block;
            position: absolute;
            width: 100%;
            height: 100%;
            background-image: -webkit-gradient(linear, left top, left bottom, from(hsla(0, 0%, 100%, 0)), to(rgba(27, 51, 83, .75)));
            background-image: -webkit-linear-gradient(top, hsla(0, 0%, 100%, 0), rgba(27, 51, 83, .75));
            background-image: linear-gradient(
                    180deg, hsla(0, 0%, 100%, 0), rgba(27, 51, 83, .75));
        }

        .images_caption {
            width: 100%;
            font-weight: 900;
            font-size: 24px;
            color: #fff;
            position: absolute;
            text-align: center;
            bottom: 0.75rem;
        }

        .owl-dots {
            display: none;
        }
        .more_mobile {
            display: none;
        }
        .carousel-item .mobile  {
            display: none;
        }

        .carousel-item .deskpot  {
            display: block;
        }
        @media screen and (min-width: 320px) and (max-width: 575px) {
            .recent__property-carousel .owl-dots {
                display: none !important;
            }
            .title__head h2::before, .title__head h2::after {
                display: none;
                text-align: left;
            }
            .title__head h2 {
            justify-content: flex-start;
        }
            .more_mobile {
                display: inline;
                font-size: 12px;
                font-weight: 900;
                justify-content: flex-end;
            }
            .featured__property {

            }

            .carousel-item .mobile  {
                display: flex;
            }

            .carousel-item .deskpot  {
                display: none !important;
            }
        }

        .figure-img {
            max-height: 145px;
        }

    </style>

