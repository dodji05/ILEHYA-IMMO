                    <div class="d-flex align-items-center">
                        <!--begin::Actions-->

                        <a href="{% block actionpath %} {% endblock %}" class="btn btn-light-primary font-weight-bolder btn-sm">Actions</a>
                        <!--end::Actions-->

                    </div>
                    <!--end::Toolbar-->
